{
  "Driver:Demo": "yay",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "Using":[
       "Serilog.Sinks.File",
       "Serilog.Sinks.Console",
       "Serilog.Sinks.Async"
    ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/On-.json",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "rollingInterval": "Day",
                "buffered": true
              }
            }
          ]
        }
      }
    ],
    "Properties": {
      "Application": "CoolCardGames.Cli"
    }
  }
}
